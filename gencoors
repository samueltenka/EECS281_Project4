def gc(num_pts,fname):
   from random import choice
   rc = lambda: str(choice(range(-1024,1024+1)))
   with open(fname,'w') as f:
      f.write(str(num_pts)+'\n')
      f.write('\n'.join(rc()+' '+rc() for i in range(num_pts)))
gc(25,'coors_25')
